<%= form_with(url: login_path, scope: :session, local: true) do |form| %>
  <div class="actions">
    <%= form.label :email %>
    <%= form.email_field :email %>
    <br>
    <%= form.label :password %>
    <%= form.password_field :password %>
    <br>
    <%= form.submit "Log in"%>
  </div>
<% end %>
